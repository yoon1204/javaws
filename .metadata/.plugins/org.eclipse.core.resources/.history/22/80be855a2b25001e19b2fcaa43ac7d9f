package algoPrj.day2;

import java.util.Scanner;

public class 김현수S_4 {

	public static void main(String[] args) {
		
		
		// 입력자료
		String name;
		int kor, eng, math;		
		
		// 출력자료
		double avg;
		double avg_kor, avg_eng, avg_math;
		
		
		// 처리자료
		int n=1; // 반복제어변수
		int sum_kor=0, sum_eng, sum_math=0;
		
		final int MAX=10; // 상수화변수

		Scanner sc = new Scanner(System.in);
		
		while(n<=MAX) {
			// 입력
			name = sc.next();
			kor = sc.nextInt();
			eng = sc.nextInt();
			math = sc.nextInt();
		}
	}

}
