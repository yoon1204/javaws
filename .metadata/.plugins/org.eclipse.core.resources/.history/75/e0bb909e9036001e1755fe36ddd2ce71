package day17;

import java.util.Scanner;

public class T {

	public static void main(String[] args) {
		String s = "ddd";
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("단어를 입력하세요: ");
		String temp = sc.nextLine();
		
		
		
		if(s.equals(temp))
			System.out.println("맞아요");
		else
			System.out.println("틀려요");
		
	}

}
